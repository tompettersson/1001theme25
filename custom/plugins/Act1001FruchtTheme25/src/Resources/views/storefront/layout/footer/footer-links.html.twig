
{% block layout_footer_social_media %}
    <div class="footer-social-media">
        <div class="row footer-links">
            {% block layout_footer_logo %}
                <div class="col-6 col-lg-12 footer-logo">
                    <a class="footer-logo-main-link"
                       href="{{ path('frontend.home.page') }}"
                       title="{{ "header.logoLink"|trans|striptags }}"
                       rel="noopener"
                       target="_blank">
                        {% block layout_header_footer_image %}
                          
                                <img src="https://www.1001frucht.de/media/2e/4d/c4/1684496552/1001fruchtFavicon.png"
                                     alt="{{ "header.logoLink"|trans|striptags }}"
                                     class="img-fluid header-logo-main-img"/>
                         
                        {% endblock %}
                    </a>
                </div>
            {% endblock %}
            {% block layout_footer_soial_links %}
                <div class="col-6 col-lg-12 footer-social_links">
                    <p style="font-size: 14px;">
                        Folgen und aktuelle Angebote nicht verpassen:
                    </p>
                    <a class="links instagram"
                       href="{{ theme_config('sw-instagram-link') |sw_encode_url }}"
                       title="{{ "footer.instagramLink"|trans|striptags }}"
                       rel="noopener"
                       target="_blank">
                        {% sw_icon 'ic-twitter' style {'pack' : 'shape'} %}
                    </a>
                    <a class="links facebook"
                       href="{{ theme_config('sw-facebook-link') |sw_encode_url }}"
                       title="{{ "footer.facebookLink"|trans|striptags }}"
                       rel="noopener"
                       target="_blank">
                        {% sw_icon 'ic-facebook' style {'pack' : 'shape'} %}
                    </a>
                    <a class="links pinterest"
                       href="https://www.pinterest.de/1001frucht/"
                       title="{{ "footer.pinterestLink"|trans|striptags }}"
                       rel="noopener"
                       target="_blank">
                        {% sw_icon 'ic-pinterest' style {'pack' : 'shape'} %}
                    </a>
                    <a class="links tiktok"
                       href="https://www.tiktok.com/@1001frucht.de"
                       title="{{ "footer.tiktokLink"|trans|striptags }}"
                       rel="noopener"
                       target="_blank">
                        <img style="width:27px;" src="data:image/png;base64," alt="TikTok">
                    </a>
                    <a class="links linkedin"
                       href="https://www.linkedin.com/company/7339015/"
                       title="LinkedIn 1001Frucht"
                       rel="noopener"
                       target="_blank">
                        <img style="width: 27px; height: 27px;" src="/media/ce/27/a4/1741073489/linkedin.svg" />
                
                    </a>
                    <a class="links reddit"
                       href="https://www.reddit.com/user/1001_Frucht/"
                       title="{{ "footer.pinterestLink"|trans|striptags }}"
                       rel="noopener"
                       target="_blank">
                        <img style="width: 30px; height: 30px;" src="/media/8b/0e/d0/1741072488/reddit.svg" />
                    </a>
                </div>
            {% endblock %}
        </div>
    </div>
{% endblock %}
